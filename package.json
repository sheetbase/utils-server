{
   "name": "@sheetbase/utils-server",
   "version": "0.0.6",
   "description": "Useful utilities for Sheetbase backend app.",
   "author": "Sheetbase",
   "homepage": "https://sheetbase.net",
   "license": "MIT",
   "main": "./dist/bundles/utils.umd.js",
   "scripts": {
      "code": "sheetbase-app-scripts build",
      "readme": "sheetbase-app-scripts readme",
      "docs": "sheetbase-app-scripts docs",
      "pre-test": "tsc --project tests/tsconfig.test.json",
      "build": "npm run code && npm run readme && npm run docs && npm run pre-test",
      "deploy": "sheetbase-app-scripts deploy",
      "lint": "tslint --project tslint.json && echo 'No lint errors.'",
      "test": "nyc --cache false mocha --timeout 100000 -- tests/out-tsc/tests/*.js",
      "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
   },
   "keywords": [
      "google-apps-script",
      "sheetbase",
      "utilities"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/sheetbase/utils-server.git"
   },
   "bugs": {
      "url": "https://github.com/sheetbase/utils-server/issues"
   },
   "dependencies": {},
   "devDependencies": {
      "@sheetbase/app-scripts": "0.0.19",
      "@types/chai": "^4.1.7",
      "@types/google-apps-script": "0.0.28",
      "@types/mocha": "^5.2.5",
      "@types/node": "^10.12.2",
      "chai": "^4.2.0",
      "coveralls": "^3.0.2",
      "mocha": "^5.2.0",
      "nyc": "^13.1.0",
      "tslint": "^5.11.0",
      "typescript": "^3.1.6"
   }
}
